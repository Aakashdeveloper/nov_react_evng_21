import React from 'react';
import ReactDOM from 'react-dom';
import Header from './header';
import Footer from './footer'

const App = () => {
    return(
        <React.Fragment>
            <Header/>
            <h1>Hii From React App</h1>
            <h2>My app</h2>
            <Footer/>
        </React.Fragment>
    )
}

ReactDOM.render(<App/>, document.getElementById('root'))


